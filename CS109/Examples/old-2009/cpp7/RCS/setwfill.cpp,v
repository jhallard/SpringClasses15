@// $Id: setwfill.cc,v 1.3 2009-02-26 16:28:35-08 - - $

// Use of set width.

#include <iomanip>
#include <iostream>

using namespace std;

int main (int argc, char **argv) {
   cout << setw(4) << setfill('0') << 20 << endl;
   cout << setw(8) << setfill('x') << 1 << endl;
   cout << 6 << endl;
   return 0;
}

